<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
  <folder name="Editors">
    <folder name="SideBar">
      <file name="de-adito-git-nbm-sidebar-EditorColorizerFactory.instance">
        <attr name="position" intvalue="1500"/>
      </file>
      <file name="de-adito-git-nbm-sidebar-AnnotatorFactory.instance">
        <attr name="position" intvalue="1400"/>
      </file>

    </folder>
  </folder>

  <folder name="Templates">
    <folder name="Project">
      <folder name="Adito">
        <file name = "aditoProjectFromRepository">
          <attr name="displayName" bundlevalue="de.adito.git.nbm.wizard.Bundle#aditoProjectFromRepository"/>
          <attr name="instantiatingIterator" newvalue="de.adito.git.nbm.wizard.AditoRepositoryCloneWizard"/>
          <attr name="template" boolvalue="true"/>
          <attr name="SystemFileSystem.icon" methodvalue="de.adito.git.nbm.wizard.AditoRepositoryCloneWizard.getProjectImage"/>
          <attr name="templateWizardURL" urlvalue="nbresloc:/de/adito/git/nbm/wizard/aditoProjectFromRepository.html"/>
        </file>
      </folder>
    </folder>
  </folder>
